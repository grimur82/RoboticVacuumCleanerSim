package sensor;

public class SurfaceFactory {
	public static Surface getSurface(String type, int x, int y){
		if(type.equals("bare")){
			return SurfaceBareImpl(type,x,y);
		}
		if(type.equals("low-pile")){
			return SurfaceLowPileImpl(type,x,y);
		}
		if(type.equals("high-pile")){
			return SurfaceHighPileImpl(type,x,y);
		}
		return null;
	}

	private static Surface SurfaceHighPileImpl(String type, int x, int y) {
		// TODO Auto-generated method stub
		if(type.equals("bare")){
			return SurfaceBareImpl(type,x,y);
		}
		return null;
	}


}
