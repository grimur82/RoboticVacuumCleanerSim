package sensor;

public class SurfaceBareImpl implements Surface{
	private int x;
	private int y;
	private String type;
	public SurfaceBareImpl(int x, int y, String t){
		this.x = x;
		this.y = y;
		this.type = t;
	}
	public int getX() {
		return x;
	}
	public int getY() {
		return y;
	}
	public String getType() {
		return type;
	}
}
