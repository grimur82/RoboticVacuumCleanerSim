package sensor;

public class TilingBedRoomImpl implements Tiling{
	private int fromX;
	private int toX;
	private int fromY;
	private int toY;
	public TilingBedRoomImpl(int tX, int fX, int tY, int fY){
		tX = toX;
		fX = fromX;
		fromY = fY;
		toY = tY;
	}
	public int getFromX() {
		// TODO Auto-generated method stub
		return 0;
	}

	public int getToX() {
		// TODO Auto-generated method stub
		return 0;
	}

	public String getType() {
		return "bedroom";
	}
	public int getFromY() {
		// TODO Auto-generated method stub
		return 0;
	}
	public int getToY() {
		// TODO Auto-generated method stub
		return 0;
	}

}
