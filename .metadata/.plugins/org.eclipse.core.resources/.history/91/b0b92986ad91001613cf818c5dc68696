package navigation;

public class CleanSweeper{

	private String dir;
	private int capacity;
	private static CleanSweeper cleanSweeper = new CleanSweeper(dir,capacity);
	private CleanSweeper(String direction, int cap){
		this.dir = direction;
		this.capacity = cap;
	}
	public static CleanSweeper getInstance(){
		return cleanSweeper;
	}
	public int getCapacity() {
		return capacity;
	}

	public String getDirection(String dir) {
		return dir;
	}

	public void shutDown() {
		// TODO Auto-generated method stub
		
	}

}
