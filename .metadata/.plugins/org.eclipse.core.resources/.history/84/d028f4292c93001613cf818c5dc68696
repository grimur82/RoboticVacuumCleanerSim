package sensor;

public class ObstacleFloorImpl implements Obstacle{
	private int x;
	private int y;
	private String type;
	public ObstacleFloorImpl(int x, int y){
		this.x = x;
		this.y = y;
	}
	public int getX() {
		return x;
	}

	public int getY() {
		return y;
	}

	public String getType() {
		return "floor";
	}
	public void setType(String t){
		
	}

}
