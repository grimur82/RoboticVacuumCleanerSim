package navigation;

public class ControlSystemLoader {
	private Navigation navigaton;
	private CleanSweeper cleanSweeper;
	private ControlSystemLoader(){
		navigaton = Navigation.getInstance();
		cleanSweeper = CleanSweeper.getInstance();
	}
	public setNavigation(int x, int y){
		navigation.setX(x);
		navigtion.setY(y);
	}
}
